<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Message" table="message">
    <indexes>
      <index name="fk_message_conversation1_idx" columns="conversation_id"/>
      <index name="fk_message_user1_idx" columns="user_user_id"/>
    </indexes>
    <id name="messageId" type="integer" column="message_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="content" type="text" column="content" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="integer" column="status" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <field name="deletedAt" type="datetime" column="deleted_at" nullable="true"/>
    <many-to-one field="conversation" target-entity="Conversation" fetch="LAZY">
      <join-columns>
        <join-column name="conversation_id" referenced-column-name="conversation_id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="userUser" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="user_user_id" referenced-column-name="user_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
