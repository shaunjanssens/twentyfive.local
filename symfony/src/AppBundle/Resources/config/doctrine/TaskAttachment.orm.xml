<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TaskAttachment" table="task_attachment">
    <indexes>
      <index name="fk_task_attachment_task1_idx" columns="task_task_id"/>
    </indexes>
    <id name="taskAttachmentId" type="integer" column="task_attachment_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="url" type="string" column="url" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <field name="deletedAt" type="datetime" column="deleted_at" nullable="true"/>
    <many-to-one field="taskTask" target-entity="Task" fetch="LAZY">
      <join-columns>
        <join-column name="task_task_id" referenced-column-name="task_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
