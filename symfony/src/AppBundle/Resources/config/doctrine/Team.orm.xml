<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Team" table="team">
    <indexes>
      <index name="fk_team_organisation1_idx" columns="organisation_id"/>
    </indexes>
    <id name="teamId" type="integer" column="team_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <field name="deletedAt" type="datetime" column="deleted_at" nullable="true"/>
    <many-to-one field="organisation" target-entity="Organisation" fetch="LAZY">
      <join-columns>
        <join-column name="organisation_id" referenced-column-name="organisation_id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="userUser" target-entity="User" inversed-by="teamTeam" fetch="LAZY">
      <join-table name="team_has_user">
        <join-columns>
          <join-column name="team_team_id" referenced-column-name="team_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="user_user_id" referenced-column-name="user_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="weblinkWeblink" target-entity="Weblink" inversed-by="teamTeam" fetch="LAZY">
      <join-table name="team_has_weblink">
        <join-columns>
          <join-column name="team_team_id" referenced-column-name="team_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="weblink_weblink_id" referenced-column-name="weblink_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
