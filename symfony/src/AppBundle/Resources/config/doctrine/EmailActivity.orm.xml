<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\EmailActivity" table="email_activity">
    <indexes>
      <index name="fk_email_activity_user1_idx" columns="user_id"/>
    </indexes>
    <id name="emailId" type="integer" column="email_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="integer" column="type" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="content" type="text" column="content" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <many-to-one field="user" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="user_id" referenced-column-name="user_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
