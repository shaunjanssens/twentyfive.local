<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\User" table="user">
    <id name="userId" type="integer" column="user_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstName" type="string" column="first_name" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="last_name" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="displayname" type="string" column="displayname" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <field name="deletedAt" type="datetime" column="deleted_at" nullable="true"/>
    <field name="roleId" type="integer" column="role_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lastLogin" type="datetime" column="last_login" nullable="true"/>
    <field name="enabled" type="boolean" column="enabled" nullable="true"/>
    <many-to-many field="conversationConversation" target-entity="Conversation" mapped-by="userUser" fetch="LAZY"/>
    <many-to-many field="teamTeam" target-entity="Team" mapped-by="userUser" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
