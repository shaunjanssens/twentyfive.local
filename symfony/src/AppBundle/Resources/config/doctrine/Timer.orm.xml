<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Timer" table="timer">
    <indexes>
      <index name="fk_timer_task1_idx" columns="task_id"/>
      <index name="fk_timer_user1_idx" columns="user_id"/>
    </indexes>
    <id name="timerId" type="integer" column="timer_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lastTime" type="integer" column="last_time" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="maxTime" type="integer" column="max_time" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lastRound" type="integer" column="last_round" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="maxRound" type="integer" column="max_round" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <field name="deletedAt" type="datetime" column="deleted_at" nullable="true"/>
    <many-to-one field="task" target-entity="Task" fetch="LAZY">
      <join-columns>
        <join-column name="task_id" referenced-column-name="task_id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="user" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="user_id" referenced-column-name="user_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
